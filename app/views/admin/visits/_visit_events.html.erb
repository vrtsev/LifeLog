<table class="table table-striped b-t">
  <thead>
    <tr>
      <th>#</th>
      <th>Событие</th>
      <th>Свойства</th>
      <th>Время</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.id %></td>
        <td><%= event[:name] %></td>
        <td><%= event[:properties] %></td>
        <td><%= format_datetime(event.time) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="text-center">
  <%= will_paginate @events, renderer: ::PaginateHelper::LinkRenderer %>
</div>